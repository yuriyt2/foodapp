

<h3> New User</h3>

<%flash.each do |message_type, message| %>
   <div class="alert"-<%= message_type %>"><h4><%= message %></h4></div>
 <% end %>

<%=form_for @user do |f| %>
<%=f.label :username %>
<%=f.text_field :username %><br>
<%=f.label :password %>
<%=f.password_field :password %><br><br>
<%=f.submit "Create User" %>

<%end %>
