<h1>Login, please</h1>


<%= form_tag '/sessions', method: :post do %>
<%= label_tag :username %>
<%= text_field_tag :username%><br>
<%= label_tag :password %>
<%= password_field_tag :password%><br>

<%= submit_tag "Log in"%>
<% end %>

<%= button_to "Create New User", new_user_path, method: :get%>
